{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": [2, { "allow": ["_id"] }],
    "no-use-before-define": ["error", { "functions": false }],
    "semi": [2, "always"]
  }
}
